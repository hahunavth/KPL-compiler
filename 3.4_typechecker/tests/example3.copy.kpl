PROGRAM  EXAMPLE3;  (* TOWER OF HANOI *)
VAR  I:INTEGER;  
     N:INTEGER;  
     P:INTEGER;  
     Q:INTEGER;
     C:CHAR;
     A:ARRAY(.5.) OF ARRAY (.5.) OF ARRAY (.5.) OF CHAR;

PROCEDURE  HANOI(N:INTEGER;  S:INTEGER;  Z:INTEGER);
BEGIN
  IF  N != 0  THEN
    BEGIN
      I:=I+1;  
      CALL  WRITELN;
      CALL  WRITEI(I);  
      CALL  WRITEI(N);
      CALL  WRITEI(S);  
      CALL  WRITEI(Z);
    END
END;  (*END OF HANOI*)

function f :integer;
BEGIN
  f := 1;
END; 

BEGIN
  FOR  N := 1  TO  4  DO  
    BEGIN
      FOR  I:= 3  TO  4  DO  
        CALL  WRITEC(' ');
      C :=  READC;  
      CALL  WRITEC(C)
    END;
  P:=1;  
  Q:=2;
  FOR  N:=2  TO  4  DO
    BEGIN  
      I:=0;  
      CALL  HANOI(N,P,Q);  
      CALL  WRITELN  
    END;

    A(.1.)(.1.)(.1.) := '1';

    I := f;

END.  (* TOWER OF HANOI *)
